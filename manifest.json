{
  "manifest_version": 3,
  "name": "Lead Magnet Extractor",
  "version": "2.0",
  "description": "Elite lead extraction tool with deep scanning, DOM highlighting, and smart obfuscation detection.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}